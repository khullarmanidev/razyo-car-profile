<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
    <table name="razoyo_customercarprofile" resource="default" engine="innodb" comment="Razoyo Customercarprofile">
        <column xsi:type="int" name="customercarprofile_id" unsigned="true" nullable="false" identity="true" comment="Customer Car Profile ID"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
            <column name="customercarprofile_id"/>
        </constraint>
        <column xsi:type="varchar" name="car_id" nullable="true" length="255" comment="Car ID"/>
        <column xsi:type="int" name="customer_id" unsigned="true" nullable="true" identity="false" comment="Customer ID"/>
    </table>
</schema>
